<apex:page controller="VJAccController" tabStyle="Account">
    
    <apex:pageBlock title="Account Listing">
		<apex:pageBlockSection columns="1">
            <apex:pageBlockTable value="{!Accounts}" var="item">
                
                <apex:column headerValue="AccName">
					<apex:outputText value="{!item.Name}"/>
				</apex:column>
                
               
				<apex:column headerValue="BillingCity">
					<apex:outputField value="{!item.billingCity}"/>
				</apex:column>
                
            </apex:pageBlockTable>   
		</apex:pageBlockSection>
	</apex:pageBlock>
    
</apex:page>