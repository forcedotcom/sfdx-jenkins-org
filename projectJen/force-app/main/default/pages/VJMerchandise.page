<apex:page standardcontroller="Account" recordSetVar="Acc"   >
   <apex:form >
    
       <apex:commandLink action="{!First}" value="First"  />  <s> </s> <apex:commandLink action="{!Next}" value="Next" />  <s> </s> <apex:commandLink action="{!Previous}" value="Previous" /> <s> </s> <apex:commandLink action="{!Last}" value="Last" />

       
    <apex:pageBlock title="Account List">
		<apex:pageBlockSection columns="1"  >
            <apex:pageBlockTable value="{!acc}" var="pitem" rows="10">
                
                
                <apex:column headerValue="AccountName">
					<apex:outputText value="{!pitem.Name}"/>
				</apex:column>
                
               
				<apex:column headerValue="Billing City">
					<apex:outputField value="{!pitem.billingcity}"/>
				</apex:column>
                
            </apex:pageBlockTable>   
		</apex:pageBlockSection>
	</apex:pageBlock>
  </apex:form> 
</apex:page>